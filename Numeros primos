num1=int(input("Ingrese el numero de inicio del intervalo: "))
num2=int(input("Ingrese el nÃºmero de fin del intervalo: "))

for prim in range (num1, num2):
    if prim > 1:
        cont=0
        i=2
        while i<prim and cont==0:
            resto=prim%i
            if resto==0:
                cont+=1
            i+=1
        if cont==0:
            print(prim)
